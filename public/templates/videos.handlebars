<fieldset>
    <legend align="center">Workout videos</legend>

    <div class="upload col-sm-12">
        <button id="video-add" class="btn btn-default btn-file"> Upload your Workout! </button>
        <input id="video-url" type="text" placeholder="Enter YouTube URL">
        <label class="radio-inline"><input type="radio" name="difficulty" id="beginnerDifficulty">Beginner</label>
        <label class="radio-inline"><input type="radio" name="difficulty" id="intermediateDifficulty">Intermediate</label>
        <label class="radio-inline"><input type="radio" name="difficulty" id="advancedDifficulty">Advanced</label>
    </div>
    <h5 class="difficultyHeader" align="center"><strong>Videos for beginner users</strong><button id="clearBeginner" type="button" class="btn btn-danger">Clear custom videos</button></h5>
    <br/>
    <div class="beginner" align="center">
        <div class="col-sm-4">
            <iframe src="https://www.youtube.com/embed/dMtgGArX1qI" frameborder="0" allowfullscreen></iframe>
        </div>
        <div class="col-sm-4">
            <iframe src="https://www.youtube.com/embed/GA-_aGqmXHc" frameborder="0" allowfullscreen></iframe>
        </div>
        <div class="col-sm-4">
            <iframe src="https://www.youtube.com/embed/wjEXO5V3skQ" frameborder="0" allowfullscreen></iframe>
        </div>
    </div>
    <br/>
    <h5 class="difficultyHeader" align="center"><strong>Videos for intermediate users</strong> <button id="clearIntermediate" type="button" class="btn btn-danger">Clear custom videos</button></h5>
    <br/>
